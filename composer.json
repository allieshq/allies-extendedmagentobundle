{
    "name": "allies/oro-extended-magento-bundle",
    "type": "symfony-bundle",
    "description": "Allies Extended Magento Bundle",
    "keywords":["OroCRM","Magento","CRM"],
    "license":"MIT",
    "require": {
        "php": ">=5.6",
        "oro/crm": ">=2.0"
    },
    "autoload": {
        "psr-0": { "Allies\\Bundle\\ExtendedMagentoBundle": "src/" }
    },
    "target-dir": "src/Allies/Bundle/ExtendedMagentoBundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
